{
  "name": "learnstorybook-design-system",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "cross-env": "^7.0.2",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "build-storybook-docs": "build-storybook -s public --docs",
    "build-storybook": "build-storybook -s public",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "lint": "eslint src --ext js",
    "format": "prettier --write ./src/**",
    "prepare": "npm run build",
    "release": "auto shipit",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-docs": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "auto": "^7.4.3",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2",
    "storybook-chromatic": "^2.2.2"
  },
  "description": "Learn Storybook design system",
  "main": "dist/index.js",
  "repository": "https://github.com/chromaui/learnstorybook-design-system.git",
  "author": "Tom Coleman <tom@thesnail.org>",
  "license": "MIT"
}
